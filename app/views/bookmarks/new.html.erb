<div class="container mt-4">
  <div class="border shadow-lg px-4 py-4">
    <%= simple_form_for [@list,@bookmark] do |f| %>
      <%= f.input :comment %>
      <%= f.association :movie %>
      <%= f.button :submit ,class: "btn-primary"  %>
    <% end %>
  </div>

  <%= simple_form_for(@list) do |f| %>
    <%= f.input :photo, as: :file %>
  <% end %>
</div>
